<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/gaaaamamemmeme"
    app:startDestination="@id/gomo1Fragment">

    <fragment
        android:id="@+id/gomo1Fragment"
        android:name="com.miHoYo.Gen.Gomo1Fragment"
        android:label="fragment_gomo1"
        tools:layout="@layout/fragment_gomo1" >
        <action
            android:id="@+id/action_gomo1Fragment_to_gomo2Fragment"
            app:destination="@id/gomo2Fragment" />
    </fragment>
    <fragment
        android:id="@+id/gomo2Fragment"
        android:name="com.miHoYo.Gen.Gomo2Fragment"
        android:label="fragment_gomo2"
        tools:layout="@layout/fragment_gomo2" >
        <action
            android:id="@+id/action_gomo2Fragment_to_gomo3Fragment"
            app:destination="@id/gomo3Fragment" />
    </fragment>
    <fragment
        android:id="@+id/gomo3Fragment"
        android:name="com.miHoYo.Gen.Gomo3Fragment"
        android:label="fragment_gomo3"
        tools:layout="@layout/fragment_gomo3" >
        <action
            android:id="@+id/action_gomo3Fragment_to_gomo1Fragment"
            app:destination="@id/gomo1Fragment"
            app:launchSingleTop="true"
            app:popUpTo="@id/gomo1Fragment"
            app:popUpToInclusive="true" />
    </fragment>
</navigation>